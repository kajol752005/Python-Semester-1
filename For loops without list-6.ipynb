   "source": [
    "Given a positive integer, find out the sum of all even numbers from 2 upto this number."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "a=int(input(\"Enter a positive integer:\"))\n",
    "sum_a=0\n",
    "for i in range(2,a+1):\n",
    "    if i%2==0:\n",
    "        print(i)\n",
    "        sum_a+=i\n",
    "print(\"sum\",\"=\",sum_a)  "
   ]
  }
